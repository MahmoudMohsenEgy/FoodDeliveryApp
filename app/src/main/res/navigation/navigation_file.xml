<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_file"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.advancedsoftwareproject.ui.homeFragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_menuFragement"
            app:destination="@id/menuFragement" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.advancedsoftwareproject.ui.searchFragment.searchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_menuFragement"
            app:destination="@id/menuFragement" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.advancedsoftwareproject.ui.cartFragment.cartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/caloriesFragment"
        android:name="com.example.advancedsoftwareproject.ui.fitnessFragment.caloriesFragment"
        android:label="fragment_calories"
        tools:layout="@layout/fragment_calories" />
    <fragment
        android:id="@+id/menuFragement"
        android:name="com.example.advancedsoftwareproject.ui.MenuFragement"
        android:label="fragment_menu_fragement"
        tools:layout="@layout/fragment_menu_fragement" />
</navigation>